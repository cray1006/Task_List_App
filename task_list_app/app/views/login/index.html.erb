
<h1>Login</h1>
<p>
<%= form_tag(authenticate_path) do %>

  <div class="field">
    <%= label_tag :username %><br>
    <%= text_field_tag :username %>
  </div><br>
  <div class="field">
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
  </div><br>
  <div class="actions">
    <%= submit_tag "Login" %>
  </div>
<% end %>
</p>
<%= link_to 'Create New User', new_user_path%>